<Window x:Class="Monitor.windows.exam_PatientModify"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
        xmlns:PA="clr-namespace:Monitor.PA"
        Title="exam_PatientModify" 
        Height="300" 
        Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <PA:PatientInformation/>
        <StackPanel Grid.Row="1">
            <Button Content="Cancel" Name="CancelBtn" Click="CancelBtn_OnClick"></Button>
            <Button Content="OK" Name="OKBtn" Click="OKBtn_OnClick"></Button>
        </StackPanel>
    </Grid>
</Window>
