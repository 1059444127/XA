<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Style x:Key="FocusVisual">
		<Setter Property="Control.Template">
			<Setter.Value>
				<ControlTemplate>
					<Rectangle SnapsToDevicePixels="true" Stroke="#B2000000" StrokeThickness="1" StrokeDashArray="1 2" Margin="1"/>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<DropShadowEffect x:Key="PanelEffect" BlurRadius="16" ShadowDepth="4" Opacity="0.35" Direction="270"/>
	<DropShadowEffect x:Key="ButtonEffect" BlurRadius="6" ShadowDepth="2" Color="#330C0E13" Opacity="0.5"/>
	<DropShadowEffect x:Key="DialogEffect" BlurRadius="6" ShadowDepth="2" Color="#33000000"/>
	<DropShadowEffect x:Key="PopupEffect" BlurRadius="12" ShadowDepth="2" Color="#4C000000"/>
	<SolidColorBrush x:Key="BtnBG" Color="#FF23333b"/>
	<SolidColorBrush x:Key="BtnBorder" Color="#198FA1AB"/>
	<SolidColorBrush x:Key="BtnHove" Color="#5c5e5f"/>
	<SolidColorBrush Color="#424445" x:Key="BtnDown"/>
	<SolidColorBrush x:Key="Highlighted" Color="#FF3B87CF"/>
	<SolidColorBrush x:Key="white" Color="#FFFFFFFF"/>
	<SolidColorBrush Color="#373a3a" x:Key="PanelContent"/>
	<SolidColorBrush x:Key="TextBG" Color="#FF242626"/>
	<LinearGradientBrush x:Key="TextBorder" EndPoint="0.5,1" StartPoint="0.5,0">
		<GradientStop Color="Transparent" Offset="0"/>
		<GradientStop Color="#33FFFFFF" Offset="1"/>
	</LinearGradientBrush>
	<SolidColorBrush x:Key="ControlColor" Color="#FF35A266"/>
	<SolidColorBrush x:Key="black0_BG" Color="#003D3F40"/>
	<SolidColorBrush x:Key="black10_Color" Color="#19000000"/>
	<SolidColorBrush x:Key="black20_Color" Color="#33000000"/>
	<SolidColorBrush x:Key="black30_Border" Color="#4C000000"/>
	<SolidColorBrush x:Key="black40_Color" Color="#66000000"/>
	<SolidColorBrush x:Key="white70_Font" Color="#B2FFFFFF"/>
	<SolidColorBrush x:Key="white5_BG" Color="#0CFFFFFF"/>
	<SolidColorBrush x:Key="white10_Color" Color="#19FFFFFF"/>
	<SolidColorBrush x:Key="white15_Color" Color="#26FFFFFF"/>
	<SolidColorBrush x:Key="white20_BG" Color="#33FFFFFF"/>
	<DataTemplate x:Key="DataTemplate">
		<Grid>
			<StackPanel Orientation="Horizontal">
				<Image Height="24" Width="24" VerticalAlignment="Center" Margin="14,0"/>
				<TextBlock Text="{TemplateBinding Content}" VerticalAlignment="Center"/>
			</StackPanel>
			<Border x:Name="border" BorderThickness="1" HorizontalAlignment="Right" Height="20" VerticalAlignment="Top" Width="20" Background="#FFDF5656" CornerRadius="10">
				<TextBlock Text="{TemplateBinding Uid}" HorizontalAlignment="Center" FontSize="14" Foreground="{DynamicResource white}"/>
			</Border>
		</Grid>
		<DataTemplate.Triggers>
			<Trigger Property="Uid" Value="0">
				<Setter Property="Visibility" TargetName="border" Value="Collapsed"/>
			</Trigger>
		</DataTemplate.Triggers>
	</DataTemplate>

