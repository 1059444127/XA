<local:XShell x:Class="UIH.XA.XSample.Shells.MainShell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:UIH.XA.XSample.Shells" xmlns:Core="clr-namespace:UIH.XA.Core;assembly=UIH.XA.XBoot"
              Title="MainShell" Height="600" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="3*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <ContentControl Core:XAppRegionRegister.RegionName="MainLeftUpRegion"  Grid.Row="0" Grid.Column="0"></ContentControl>
        <ContentControl Core:XAppRegionRegister.RegionName="MainLeftDownRegion"  Grid.Row="1" Grid.Column="0"></ContentControl>
        <ContentControl Core:XAppRegionRegister.RegionName="MainRightUpRegion" Grid.Row="0" Grid.Column="1"></ContentControl>
        <TabControl Core:XAppRegionRegister.RegionName="MainRightDownRegionNavigate"   Grid.Row="1" Grid.Column="1"></TabControl>
    </Grid>
</local:XShell>
